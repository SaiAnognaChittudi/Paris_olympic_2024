{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sa-paris-olympic-2024df"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"athletesHTTP_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'athletesHTTP'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://parisolympic2024dev001.dfs.core.windows.net/"
		},
		"athletesHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/SaiAnognaChittudi/Paris_olympic_2024/refs/heads/main/data/athletes.csv"
		},
		"athletesHTTP_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SaiAnognaChittudi"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sa-paris-olympic-2024df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"athletesHTTP_password": {
						"value": "[parameters('athletesHTTP_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"athletesHTTP_properties_typeProperties_url": {
						"value": "[parameters('athletesHTTP_properties_typeProperties_url')]"
					},
					"athletesHTTP_properties_typeProperties_userName": {
						"value": "[parameters('athletesHTTP_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}